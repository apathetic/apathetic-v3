{
  "name": "weshatch",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@apatheticwes/scrollify": "^0.5.1",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@snowpack/plugin-postcss": "^1.1.0",
    "@snowpack/plugin-run-script": "^2.3.0",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.1.0",
    "dayjs": "^1.8.35",
    "markdown-it": "^8.4.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "prettier": "^2.2.1",
    "rollup": "^1.29.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.1.3",
    "snowpack": "^3.0.7",
    "tailwindcss": "^2.0.2",
    "three": "^0.124.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "prettier --check \"src/**/*.js\"",
    "build": "NODE_ENV=production run-s clean build:*",
    "build:11ty": "eleventy --quiet",
    "build:rollup": "rollup -c config/rollup.config.js",
    "dev": "run-p clean dev:rollup dev:11ty",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:rollup": "rollup -w -c config/rollup.config.js",
    "start": "npm run dev",
    "startx": "snowpack dev --config config/snowpack.config.js",
    "buildx": "snowpack build --config config/snowpack.config.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
