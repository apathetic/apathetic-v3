{
  "name": "weshatch",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@apatheticwes/scrollify": "^1.0.5-alpha",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.1.0",
    "dayjs": "^1.8.35",
    "markdown-it": "^8.4.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "prettier": "^2.2.1",
    "rollup": "^3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-workbox": "^6.2.0",
    "tailwindcss": "^3.3.0",
    "three": "^0.124.0",
    "web-vitals": "^1.1.0",
    "workbox-core": "^6.1.0",
    "workbox-expiration": "^6.1.0",
    "workbox-precaching": "^6.1.0",
    "workbox-routing": "^6.1.0",
    "workbox-strategies": "^6.1.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "prettier --check \"src/**/*.js\"",
    "build": "NODE_ENV=production run-s clean build:*",
    "build:11ty": "eleventy --quiet",
    "build:js": "rollup -c config/rollup.config.js --bundleConfigAsCjs",
    "dev": "run-p dev:js dev:11ty",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:js": "rollup -w -c config/rollup.config.js --bundleConfigAsCjs",
    "start": "npm run clean && npm run dev"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
